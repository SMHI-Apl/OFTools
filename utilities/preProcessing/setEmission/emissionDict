FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      emissionDict;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

// density of air at simulation conditions
densityAir      densityAir [1 -3 0 0 0 0 0] 1.225;

// Turbulent Schmidt-number
SC_T            SC_T [0 0 0 0 0 0 0] 0.4;

// fraction of NOx directly emitted as NO2
fd              0.07;

// Mixing timescale [s]
tau             tau 100;

